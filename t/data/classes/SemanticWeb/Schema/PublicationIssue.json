         "rdfs:label" : "pagination",
         "schema:domainIncludes" : [
            "schema:PublicationVolume",
            "schema:Article",
            "schema:PublicationIssue",
            "schema:Chapter"
         ],
         "schema:rangeIncludes" : "schema:Text"
      }
   }
